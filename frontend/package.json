{
  "name": "appointment-frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "start": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:accessibility": "node src/testing/run-accessibility-tests.js",
    "test:cross-browser": "playwright test --project=chromium --project=firefox --project=webkit",
    "test:mobile": "playwright test --project='Mobile Chrome' --project='Mobile Safari'",
    "test:performance": "node src/testing/run-performance-tests.js",
    "test:all": "npm run test:unit && npm run test:e2e && npm run test:accessibility && npm run test:performance",
    "test:ci": "npm run test:unit && npm run test:e2e -- --reporter=line && npm run test:accessibility",
    "codegen": "playwright codegen",
    "show-report": "playwright show-report"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.1.7",
    "axios": "^1.9.0",
    "date-fns": "^4.1.0",
    "formik": "^2.4.6",
    "jwt-decode": "^4.0.0",
    "pwa": "^1.3.0",
    "react": "^19.1.0",
    "react-datepicker": "^8.3.0",
    "react-dom": "^19.1.0",
    "react-hot-toast": "^2.5.2",
    "react-modal": "^3.16.3",
    "react-router-dom": "^7.6.0",
    "react-toastify": "^11.0.5",
    "socket.io-client": "^4.8.1",
    "tailwindcss": "^4.1.7",
    "vite-plugin-pwa": "^1.2.0",
    "workbox-window": "^7.4.0",
    "yup": "^1.6.1"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.10.0",
    "@axe-core/react": "^4.11.0",
    "@eslint/js": "^9.25.0",
    "@playwright/test": "^1.52.0",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@vitejs/plugin-react": "^4.4.1",
    "autoprefixer": "^10.4.21",
    "axe-core": "^4.11.0",
    "eslint": "^9.25.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "lighthouse": "^12.2.2",
    "puppeteer": "^24.0.0",
    "vite": "^6.3.5",
    "vitest": "^4.0.16",
    "vitest-axe": "^0.1.0"
  }
}
